import java.util.*;

interface AdvancedArithmetic {
    int divi_sum(int n);
}

class MyCalculator implements AdvancedArithmetic {
    public int divi_sum(int n) {
        int sum = 0;
        for (int i = 1; i <= n; i++) {
            if (n % i == 0) {
                sum += i;
            }
        }
        return sum;
    }
}
